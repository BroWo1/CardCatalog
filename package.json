{
  "name": "CardCatalog",
  "version": "1.0.0",
  "description": "Hello World!",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env RENDERER_URL=http://localhost:3333 electron .",
    "dev:renderer": "npx nuxi dev src/renderer --port 3333",
    "build:renderer": "cross-env NUXT_APP_BASE_URL=./ NUXT_APP_BUILD_ASSETS_DIR=_nuxt/ npx nuxi generate src/renderer",
    "download-models": "node scripts/download-clip-model.js",
    "prepackage": "npm run download-models",
    "premake": "npm run download-models",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Will Li",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "cross-env": "^7.0.3",
    "electron": "^39.1.2",
    "nuxt": "^4.2.1"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "chart.js": "^4.5.1",
    "drivelist": "^12.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "exifr": "^7.1.3",
    "jimp": "^1.6.0",
    "rbush": "^4.0.1",
    "sql.js": "^1.13.0",
    "vue-chartjs": "^5.3.3"
  }
}
